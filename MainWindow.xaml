<Window x:Class="Курсовая_работа_сокрытие_информации.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Курсовая_работа_сокрытие_информации"
        mc:Ignorable="d"
        Title="MainWindow" 
        WindowStyle="None"
        MinHeight="500" 
        MinWidth="850"
        Height="521.96" 
        Width="856"
        WindowStartupLocation ="CenterScreen"
        AllowsTransparency="True"
        Background="Transparent"
        >
    <Border MouseDown="MoveWindow" x:Name="AssociatedObject" BorderThickness="3" CornerRadius="12" MouseLeftButtonDown="Border_MouseLeftButtonDown" BorderBrush="#FF328057">
        <Border.Background>
            <SolidColorBrush Color="#FF134206" Opacity="0.7"/>
        </Border.Background>
        <Frame x:Name="frame" NavigationUIVisibility ="Hidden">
        </Frame>
    </Border>
</Window>
